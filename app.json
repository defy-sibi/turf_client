{
  "expo": {
    "name": "Turf",
    "displayName": "turf",
    "description": "Satellite tracking app",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/alogo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/turf_splash.png",
